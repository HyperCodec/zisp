(var cache (createTable))
(def fibonacci (n) (
    (if (or (eq n 0) (eq n 1)) (
        (return n)
    ))

    (if (has cache n) (
        (return (kget cache n))
    ))

    (var "v" (- (fibonacci (- n 1)) (fibonacci (- n 2))))

    (set cache n v)

    (return v)
))

(def fibSeq (depth) (
    (if (eq depth 30) (return))

    (print depth)
    (print ": ")
    (println (fibonacci depth))

    (fibSeq (+ depth 1))
))

(fibSeq 0)